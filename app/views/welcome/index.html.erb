<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>

<div>
  <% if current_user %>
    Signed in as <strong><%= current_user.name %></strong>!
    <%= link_to "Sign out", signout_path, id: "sign_out" %>
  <% else %>
    <%= link_to "Sign in with Github", "/auth/github", id: "sign_in" %>
  <% end %>
</div>
